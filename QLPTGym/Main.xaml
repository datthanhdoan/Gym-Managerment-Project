<Window x:Class="QLPTGym.Main"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QLPTGym"
        mc:Ignorable="d"
        Title="Main" Height="450" Width="800"
        WindowStartupLocation="CenterScreen"
        AllowsTransparency="True"
        WindowStyle="None"
        MouseLeftButtonDown="Window_Move"
        >
    <Grid>
        <WrapPanel Orientation="Vertical">
            <Button Width="100" Height="50" Margin="0,35,0,35" Click="BtnNV" Content="Nhân viên"/>
            <Button Width="100" Height="50" Margin="0,0,0,35" Click="BtnKH">Khách hàng</Button>
            <Button Width="100" Height="50" Margin="0,0,0,35" Click="BtnGT">Gói tập</Button>
            <Button Width="100" Height="50" Margin="0,0,0,35" Click="BtnDT" Content="Doanh Thu"/>
            <Button Width="100" Height="30"  Click="BtnDong">Thoát</Button>
        </WrapPanel>
        <Frame x:Name="MainUI" Margin="130,0,0,0" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
